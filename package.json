{
  "name": "ABuilder",
  "version": "0.0.0",
  "author": {
    "name": "arronqzy"
  },
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "turbo run dev",
    "devBuilder": "turbo run dev --filter BuilderApp",
    "buildBuilder": "turbo run build --filter BuilderApp",
    "clean": "turbo run clean",
    "reinstall": "pnpm run clean && rm -rf node_modules && rm -rf pnpm-lock.yaml && pnpm run bootstrap",
    "lint": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
    "format": "prettier . --write --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.0",
    "prettier": "^2.5.1",
    "turbo": "^1.8.3"
  },
  "husky": {},
  "engines": {
    "node": ">=16.00",
    "npm": ">=9.0.0"
  },
  "packageManager": "pnpm@7.27.0"
}
